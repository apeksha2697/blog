<div class="container">
<% if current_user %><br>
  <%= render 'pagelayout' %>
  <%= render 'list' %>
<% else %>
<div class="jumbotron">
  <h1>Hello, Rails!</h1>
  <p>Log In or Sign Up to continue</p>
  <a class="btn btn-outline-primary" <%= link_to 'Log In', new_user_session_path %>
  </a>
  <a class="btn btn-outline-success" <%= link_to 'Sign Up', new_user_registration_path %>
  </a>
</div>  
<% end %>